{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm turbo build --filter=@mvppir/web",
  "devCommand": "pnpm turbo dev --filter=@mvppir/web",
  "installCommand": "pnpm install --filter @mvppir/web --filter @mvppir/shared-types --filter @mvppir/typescript-config",
